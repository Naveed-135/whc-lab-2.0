 <style>
        body {
            font-family: Arial, sans-serif;
            margin: 0;
            padding: 0;
        }
        
        .footer {
            background-color: #f5f5f5;
            padding: 40px 20px;
            color: #333;
        }
        
        .footer-top {
            max-width: 1200px;
            margin: 0 auto;
            display: flex;
            justify-content: space-between;
            align-items: flex-start;
            padding-bottom: 30px;
            border-bottom: 1px solid #ddd;
        }
        
        .cta {
            font-size: 24px;
            font-weight: bold;
            margin-bottom: 15px;
        }
        
        .contact-link {
            color: #0066cc;
            text-decoration: none;
            font-weight: bold;
        }
        
        .contact-link:hover {
            text-decoration: underline;
        }
        
        .contact-email {
            margin-top: 5px;
            font-size: 14px;
        }
        
        .footer-columns {
            max-width: 1200px;
            margin: 30px auto 0;
            display: flex;
            justify-content: space-between;
        }
        
        .column {
            flex: 1;
            padding: 0 15px;
        }
        
        .column h4 {
            margin-bottom: 15px;
            font-size: 16px;
        }
        
        .column ul {
            list-style: none;
            padding: 0;
            margin: 0;
        }
        
        .column li {
            margin-bottom: 10px;
            font-size: 14px;
        }
        
        .footer-bottom {
            max-width: 1200px;
            margin: 30px auto 0;
            text-align: center;
            font-size: 12px;
            color: #666;
            padding-top: 20px;
            border-top: 1px solid #ddd;
        }
    </style>

 <footer class="footer">
        <div class="footer-top">
            <div class="cta">Get started with a free consultation</div>
            <div class="contact">
                <a href="#" class="contact-link">Get in Touch →</a>
                <div class="contact-details">
                    <div class="contact-email">Left's talk</div>
                    <div class="contact-email">sales@whclab.com</div>
                </div>
            </div>
        </div>
        
        <div class="footer-columns">
            <div class="column">
                <h4>Home</h4>
                <ul>
                    <li>About</li>
                    <li>Solutions</li>
                    <li>Contact Us</li>
                </ul>
            </div>
            
            <div class="column">
                <h4>Research</h4>
                <ul>
                    <li>Manufacturing</li>
                    <li>Sustainability</li>
                    <li>Careers</li>
                </ul>
            </div>
            
            <div class="column">
                <h4>Privacy Policy</h4>
                <ul>
                    <li>Terms & Conditions</li>
                    <li>Jakarta – Indonesia</li>
                </ul>
            </div>
        </div>
        
        <div class="footer-bottom">
            <p>Copyright © 2025 who-lab group | Designed by Developios</p>
        </div>
    </footer>

{% schema %}
{
    "name": "Footer",
    "settings": [
        {
            "type": "image_picker",
            "id": "footer_logo",
            "label": "Footer Logo"
        },
        {
            "type": "textarea",
            "id": "company_description",
            "label": "Company Description",
            "default": "WHC Lab is a leading provider of innovative solutions for sustainable development and technological advancement."
        },
        {
            "type": "link_list",
            "id": "footer_menu_1",
            "label": "Quick Links Menu",
            "default": "footer"
        },
        {
            "type": "link_list",
            "id": "footer_menu_2",
            "label": "Solutions Menu",
            "default": "footer"
        },
        {
            "type": "textarea",
            "id": "address",
            "label": "Address",
            "default": "123 Business Street\nJakarta, Indonesia"
        },
        {
            "type": "text",
            "id": "email",
            "label": "Email",
            "default": "sales@whclab.com"
        },
        {
            "type": "text",
            "id": "phone",
            "label": "Phone",
            "default": "+62 123 456 789"
        },
        {
            "type": "text",
            "id": "footer_credit",
            "label": "Footer Credit",
            "default": "Designed by Developios"
        },
        {
            "type": "url",
            "id": "facebook_link",
            "label": "Facebook Link"
        },
        {
            "type": "url",
            "id": "linkedin_link",
            "label": "LinkedIn Link"
        },
        {
            "type": "url",
            "id": "instagram_link",
            "label": "Instagram Link"
        },
        {
            "type": "url",
            "id": "twitter_link",
            "label": "Twitter Link"
        },
        {
            "type": "url",
            "id": "privacy_policy_link",
            "label": "Privacy Policy Link"
        },
        {
            "type": "url",
            "id": "terms_link",
            "label": "Terms & Conditions Link"
        },
        {
            "type": "text",
            "id": "location",
            "label": "Location",
            "default": "Jakarta - Indonesia"
        }
    ],
    "presets": [
        {
            "name": "Footer",
            "category": "Footer"
        }
    ]
}
{% endschema %}